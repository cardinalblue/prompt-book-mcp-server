{
  "name": "prompt-book-mcp-server",
  "version": "1.0.1",
  "main": "build/index.js",
  "type": "module",
  "bin": {
    "prompt-book-mcp-server": "./build/index.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
