  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.0",
    "@notionhq/client": "^2.2.16",
    "@types/uuid": "^10.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "typescript": "^5.7.3"
  }
}
